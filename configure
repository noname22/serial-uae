      echo "Did not find gtk+, disabling user interface"
      GTK_CFLAGS=
      if [ "x$WANT_UI" = "xyes" ]; then
        NR_ERRORS=`expr $NR_ERRORS + 1`
      fi
    else
      echo "Using gtk+ GUI."
