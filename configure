      GUIOBJS_IN="gtkui.o util.o"
    fi
  else
    if [ "x$WANT_UI" = "xyes" ]; then
      echo "GUI requested, but no suitable libraries found."
      NR_ERRORS=`expr $NR_ERRORS + 1`
    fi
  fi
  fi
  fi
  fi
fi



